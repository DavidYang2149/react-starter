{
  "name": "@davidyang2149/react-starter",
  "version": "1.0.3",
  "description": "React starter with TypeScript, Jest, Testing-Library, Cypress",
  "bin": {
    "@davidyang2149/react-starter": "./bin/react-starter-generator.js"
  },
  "scripts": {
    "start": "vite --host",
    "dev": "vite --host localhost --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "test": "jest --watchAll --coverage --detectOpenHandles",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "cypress open",
    "e2e:headless": "cypress run"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.197",
    "@swc/jest": "^0.2.21",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^27.5.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^2.0.1",
    "cypress": "^10.7.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2",
    "vite": "^3.0.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidYang2149/react-starter.git"
  },
  "keywords": [
    "React",
    "react",
    "React starter",
    "react starter",
    "starter",
    "typescript",
    "eslint",
    "jest",
    "testing-library",
    "cypress"
  ],
  "author": "davidyang2149",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DavidYang2149/react-starter/issues"
  },
  "homepage": "https://github.com/DavidYang2149/react-starter#readme"
}
